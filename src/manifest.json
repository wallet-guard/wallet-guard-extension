{
  "manifest_version": 3,
  "name": "Wallet Guard: Protect Your Crypto",
  "key": "",
  "description": "Wallet Guard acts as a security companion to your crypto wallet of choice, so you can browse at ease.",
  "version": "1.1.3",
  "icons": {
    "128": "images/wg_logos/logo_128x128.png"
  },
  "offline_enabled": false,
  "externally_connectable": {
    "matches": ["https://*.walletguard.app/*"]
  },
  "action": {
    "default_icon": "images/wg_logos/logo_16x16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content-scripts/contentScripts.js", "js/content-scripts/bypassCheck.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["js/injected/injectWalletGuard.js", "js/vendor.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["storage", "tabs", "alarms", "management", "notifications", "contextMenus"],
  "host_permissions": ["<all_urls>"],
  "commands": {
    "run-chatweb3": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Open ChatWeb3"
    }
  }
}
