{
  "manifest_version": 3,
  "name": "Wallet Guard: Protect Your Crypto",
  "key": "",
  "description": "Wallet Guard acts as a security companion to your crypto wallet of choice, so you can browse at ease.",
  "version": "0.8.5",
  "icons": {
    "128": "images/wg_logos/logo_128x128.png"
  },
  "offline_enabled": false,
  "options_ui": {
    "page": "dashboard.html",
    "open_in_tab": true
  },
  "action": {
    "default_icon": "images/wg_logos/logo_16x16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content-scripts/contentScripts.js", "js/content-scripts/bypassCheck.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["js/injected/injectWalletGuard.js", "js/vendor.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["storage", "tabs", "alarms", "management", "notifications"],
  "host_permissions": ["<all_urls>"]
}
